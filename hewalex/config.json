{
  "name": "Hewalex2MQTT",
  "version": "0.1.1",
  "slug": "hewalex",
  "description": "Hewalex heat pump monitoring",
  "url": "https://github.com/EduardWitteveen/hewalex2mqtt-homeassistant-add-on/",
  "arch": ["armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "init": "false",
  "boot": "auto",
  "host_network": "true",
  "options": {
        "mqtt_ip": "192.168.1.5",
    	"mqtt_port": "1883",
		"mqtt_authentication": "true",
        "mqtt_user": "mqtt",
        "mqtt_pass": "homeassistant",
    	"Device_Zps_Enabled": "false",
    	"Device_Zps_Address": "192.168.1.7",
    	"Device_Zps_Port": "8899",
		"Device_Zps_MqttTopic": "SolarBoiler",
    	"Device_Pcwu_Enabled": "true",
    	"Device_Pcwu_Enabled": "192.168.1.16",
    	"Device_Pcwu_Port": "8899",
		"Device_Pcwu_MqttTopic": "Heatpump"
    },
  "schema": {
		"mqtt_ip": "url",
    	"mqtt_port": "port",
		"mqtt_authentication": "bool",
    	"mqtt_user": "str?",
    	"mqtt_pass": "str?",
    	"Device_Zps_Enabled": "bool",
    	"Device_Zps_Address": "url?",
    	"Device_Zps_Port": "port?",
   		"Device_Zps_MqttTopic": "str?",
    	"Device_Pcwu_Enabled": "bool",
    	"Device_Pcwu_Address": "url?",
    	"Device_Pcwu_Port": "port?",
  		"Device_Pcwu_MqttTopic": "str?"
    }
}
